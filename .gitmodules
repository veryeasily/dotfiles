[submodule "root/.tmux/plugins/tpm"]
	path = root/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm.git
	branch = master
[submodule "root/.config/awesome"]
	path = root/.config/awesome
	url = https://github.com/veryeasily/awesome-dotfiles
	branch = master
[submodule "root/.rbenv"]
	path = root/.rbenv
	url = https://github.com/rbenv/rbenv
[submodule "root/.pyenv"]
	path = root/.pyenv
	url = https://github.com/pyenv/pyenv
