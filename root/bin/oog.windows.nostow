search=""
echo "Googling: $@"
for term in $@; do
search="$search%20$term"
done

OOG_PATH="http:\/\/www.google.com\/search?q=$search"

# /mnt/c/Windows/System32/cmd.exe /c start chrome /new-window "$OOG_PATH"
/mnt/c/Windows/System32/cmd.exe /c start chrome "$OOG_PATH"
