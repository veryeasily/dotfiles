#!/bin/bash
##
# Borrowed from
# https://superuser.com/a/47216/157200
#
search=""
echo "Googling: $@"
for term in $@; do
search="$search%20$term"
done

OOG_PATH="http:\/\/www.google.com\/search?q=$search"
CHROME_COMMAND="/mnt/c/Windows/System32/cmd.exe /c 'start chrome /new-window "

/mnt/c/Windows/System32/cmd.exe /c 'start chrome /new-window '"$OOG_PATH"
